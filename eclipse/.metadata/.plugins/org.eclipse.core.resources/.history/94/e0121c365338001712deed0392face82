		CommandInfoAPI.registerCommandInfo(new CommandListPlayers(), "commands.players.usage", Library.Info.LIST_PLAYERS);
		CommandInfoAPI.registerCommandInfo(new CommandListBans(), "commands.banlist.usage", Library.Info.LIST_BANS);
		CommandInfoAPI.registerCommandInfo(new CommandEmote(), "commands.me.usage", Library.Info.EMOTE);
