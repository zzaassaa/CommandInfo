		CommandInfoAPI.registerCommandInfo(new CommandSetDefaultSpawnpoint(), "commands.setworldspawn.usage", Library.Info.SET_DEFAULT_SPAWNPOINT);
		CommandInfoAPI.registerCommandInfo(new CommandSetBlock(), "commands.setblock.usage", Library.Info.SET_BLOCK);
		CommandInfoAPI.registerCommandInfo(new CommandScoreboard(), "commands.scoreboard.usage", Library.Info.SCOREBOARD);
		CommandInfoAPI.registerCommandInfo(new CommandSaveOn(), "commands.save-on.usage", Library.Info.SAVE_ON);
		CommandInfoAPI.registerCommandInfo(new CommandSaveOff(), "commands.save-off.usage", Library.Info.SAVE_OFF);
