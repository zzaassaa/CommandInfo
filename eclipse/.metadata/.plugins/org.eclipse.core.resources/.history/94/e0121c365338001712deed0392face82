public class InitCommands {
	
	public static void init(){
		CommandInfoAPI.registerCoreCommand(new CmdInfo());
		
		CommandInfoAPI.registerCommandInfo(new CommandTP(), "commands.tp.usage", Library.Info.TP);
		CommandInfoAPI.registerCommandInfo(new CommandTrigger(), "commands.trigger.usage", Library.Info.TRIGGER);
		CommandInfoAPI.registerCommandInfo(new CommandToggleDownfall(), "commands.downfall.usage", Library.Info.TRIGGER);
		CommandInfoAPI.registerCommandInfo(new CommandTitle(), "commands.title.usage", Library.Info.TITLE);
		CommandInfoAPI.registerCommandInfo(new CommandTime(), "commands.time.usage", Library.Info.TIME);
		CommandInfoAPI.registerCommandInfo(new CommandStopSound(), "commands.stopsound.usage", Library.Info.STOP_SOUND);
		CommandInfoAPI.registerCommandInfo(new CommandStats(), "commands.stats.usage", Library.Info.STATS);
		CommandInfoAPI.registerCommandInfo(new CommandSpreadPlayers(), "commands.spreadplayers.usage", Library.Info.SPREAD_PLAYERS);
		CommandInfoAPI.registerCommandInfo(new CommandShowSeed(), "commands.seed.usage", Library.Info.SHOW_SEED);
		CommandInfoAPI.registerCommandInfo(new CommandSetSpawnpoint(), "commands.spawnpoint.usage", Library.Info.SET_SPAWNPOINT);
		CommandInfoAPI.registerCommandInfo(new CommandLocate(), "commands.locate.usage", Library.Info.LOCATE);
		CommandInfoAPI.registerCommandInfo(new CommandSetPlayerTimeout(), "commands.setidletimeout.usage", Library.Info.SET_PLAYER_TIMEOUT);
		CommandInfoAPI.registerCommandInfo(new CommandServerKick(), "commands.kick.usage", Library.Info.SERVER_KICK);
		CommandInfoAPI.registerCommandInfo(new CommandReplaceItem(), "commands.replaceitem.usage", Library.Info.REPLACE_ITEM);
		CommandInfoAPI.registerCommandInfo(new CommandPlaySound(), "commands.playsound.usage", Library.Info.PLAY_SOUND);
		CommandInfoAPI.registerCommandInfo(new CommandParticle(), "commands.particle.usage", Library.Info.PARTICLE);
		CommandInfoAPI.registerCommandInfo(new CommandKill(), "commands.kill.usage", Library.Info.KILL);
		CommandInfoAPI.registerCommandInfo(new CommandHelp(), "commands.help.usage", Library.Info.HELP);
		CommandInfoAPI.registerCommandInfo(new CommandGive(), "commands.give.usage", Library.Info.GIVE);
		CommandInfoAPI.registerCommandInfo(new CommandGameRule(), "commands.gamerule.usage", Library.Info.GAME_RULE);
		CommandInfoAPI.registerCommandInfo(new CommandGameMode(), "commands.gamemode.usage", Library.Info.GAME_MODE);
		CommandInfoAPI.registerCommandInfo(new CommandFill(), "commands.fill.usage", Library.Info.FILL);
		CommandInfoAPI.registerCommandInfo(new CommandExecuteAt(), "commands.execute.usage", Library.Info.EXECUTE_AT);
		CommandInfoAPI.registerCommandInfo(new CommandEntityData(), "commands.entitydata.usage", Library.Info.ENTITY_DATA);
		CommandInfoAPI.registerCommandInfo(new CommandEnchant(), "commands.enchant.usage", Library.Info.ENCHANT);
		CommandInfoAPI.registerCommandInfo(new CommandEffect(), "commands.effect.usage", Library.Info.EFFECT);
		CommandInfoAPI.registerCommandInfo(new CommandDifficulty(), "commands.difficulty.usage", Library.Info.DIFFICULTY);
		CommandInfoAPI.registerCommandInfo(new CommandDefaultGameMode(), "commands.defaultgamemode.usage", Library.Info.DEFAULT_GAME_MODE);
		CommandInfoAPI.registerCommandInfo(new CommandDebug(), "commands.debug.usage", Library.Info.DEBUG);
		CommandInfoAPI.registerCommandInfo(new CommandCompare(), "commands.compare.usage", Library.Info.COMPARE);
		CommandInfoAPI.registerCommandInfo(new CommandClone(), "commands.clone.usage", Library.Info.CLONE);
		CommandInfoAPI.registerCommandInfo(new CommandClearInventory(), "commands.clear.usage", Library.Info.CLEAR_INVENTORY);
		CommandInfoAPI.registerCommandInfo(new CommandBlockData(), "commands.blockdata.usage", Library.Info.BLOCK_DATA);
		CommandInfoAPI.registerCommandInfo(new CommandWhitelist(), "commands.whitelist.usage", Library.Info.WHITELIST);
		CommandInfoAPI.registerCommandInfo(new CommandTestForBlock(), "commands.testforblock.usage", Library.Info.TEST_FOR_BLOCK);
