		CommandInfoAPI.registerCommandInfo(new CommandWorldBorder(), "commands.worldborder.usage", Library.Info.WORLD_BORDER);
		CommandInfoAPI.registerCommandInfo(new CommandXP(), "commands.xp.usage", Library.Info.XP);
		
