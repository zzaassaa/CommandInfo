# Configuration file

category {

    general {

        info {
            I:info_threshold=0
        }

    }

}


general {
    I:info_threshold=0

    commandinfo {
        # commandinfo.info_permission.comment [range: 0 ~ 3, default: 0]
        I:cmd_info_permission=0

        # The permission level, aka the gamemode, for allowing the player to use the command, commandInfo [range: 0 ~ 3, default: 0]
        I:cmd_initial_permission=0
    }

}


